{
  "name": "doxyjs",
  "version": "1.3.1",
  "description": "Converts Javascript into psudo C++ for Doxygen",
  "main": "index.js",
  "author": "Dmitry Toropchin",
  "license": "MIT",
  "homepage": "https://github.com/dmitrytoropchin/doxyjs",
  "bugs": {
    "url": "https://github.com/dmitrytoropchin/doxyjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dmitrytoropchin/doxyjs.git"
  },
  "dependencies": {
    "commander": "^2.20.0",
    "localize": "^0.4.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "eslint": "^6.1.0",
    "jest": "^24.8.0"
  },
  "bin": {
    "doxyjs": "dist/index.js"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "cli",
    "doxygen"
  ],
  "scripts": {
    "build": "babel src --out-dir dist --source-maps inline --copy-files",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "jest"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/*.test.js"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!**/seeds/**",
      "!**/node_modules/**",
      "!**/coverage/**"
    ]
  }
}
